<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= cons1 %></p>
<% blog.forEach(function(bl) { %>
    <h3><%= bl.title %></h3>
    <p><%= bl.postName.substring(0, 100) %></p>
    <% if (bl.postName.length > 100) { %>
        <p>... <a href="/posts/<%= bl.title %>">Read More</a></p>
    <% } %>
<% }) %>




<%- include("partials/footer") -%>